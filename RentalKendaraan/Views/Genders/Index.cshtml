@model IEnumerable<RentalKendaraan.Models.Gender>

@{
    ViewData["Title"] = "Index";
}

<h1>Gender</h1>

<p>
    <a asp-action="Create">Tambah Data</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayName("Nama Gender")
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.NamaGender)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.IdGender"><i class="fa fa-marker fa-md"></i></a> |
                    <a asp-action="Details" asp-route-id="@item.IdGender"><i class="fa fa-list"></i></a> |
                    <a asp-action="Delete" asp-route-id="@item.IdGender"><i class="fa fa-trash-alt fa-md"></i></a>
                </td>
            </tr>
        }
    </tbody>
</table>
