@model RentalKendaraan.Models.Peminjaman

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Peminjaman</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayName("Tanggal Peminjaman")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TglPeminjamaan)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayName("Biaya")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Biaya)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayName("Nama Customer")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdCustomerNavigation.NamaCustomer)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayName("Jaminan")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdJaminanNavigation.NamaJaminan)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayName("Jenis Kendaraan")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdKendaraanNavigation.NamaKendaraan)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.IdPeminjamaan">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
